{
  "name": "08.quiz",
  "version": "1.0.0",
  "description": "A quiz app using Nuxt, Sockets and Vue",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev__:client": "cd app && npm run dev:client",
    "dev:sockets": "nodemon ./sockets/index.ts",
    "dev:server": "cd server && npm run dev",
    "dev:open": "node ./open.mjs"
  },
  "author": "Joran Quinten <joran@joranquinten.nl>",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^24.3.0",
    "chalk": "^5.6.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "open": "^10.2.0",
    "ts-node": "^10.9.2"
  }
}
