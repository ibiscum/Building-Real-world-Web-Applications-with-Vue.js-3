  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.34.0",
    "eslint-plugin-vue": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2",
    "vite": "^7.1.3",
    "vue-tsc": "^3.0.6"
  }
